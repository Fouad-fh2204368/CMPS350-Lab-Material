// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model account {
  accountNo    String @id @default(cuid())
  firstname    String
  lastname     String
  acctType     String
  balance      Float
  email        String
  dateOpened   String
  gender       String
  profileImage String
}



